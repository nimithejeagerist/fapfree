{
  "manifest_version": 3,
  "name": "Fapfree",
  "version": "1.0",
  "description": "A Chrome extension to block pornographic websites.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
