{
  "manifest_version": 3,
  "name": "Fapfree",
  "version": "1.0",
  "description": "A Chrome extension to block pornographic websites.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
